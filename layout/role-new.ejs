<div class="modal-content">
    <div class="modal-header">
      <h5 class="modal-title">新增</h5>
      <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
    </div>
    <div class="modal-body">
      <form action="./" method="post" id="framework_form">
        <!-- 角色名稱 -->
        <div class="col mb-3">
            <label class="form-label">角色名稱</label>
            <input type="text" class="form-control" name="input-name" placeholder="請輸入角色名稱"/>
        </div>
        <!-- 使用者 -->
          <div class="mb-3">
            <label for="add-user" class="form-label">使用者</label>
              <select class="form-select" id="add-user" data-placeholder="選擇使用者" multiple>
              <option value="user_01">總經理</option>
              <option value="user_02">王經理</option>
              <option value="user_03">任經理</option>
              <option value="user_04">江處長</option>
              <option value="user_05">謝經理</option>
              <option value="user_06">副廠長</option>
              <option value="user_07">劉處長</option>
              <option value="user_08">柯處長</option>
              <option value="user_09">楊經理</option>
              <option value="user_10">古副理</option>
              </select>
          </div>
          <!-- 指標資訊 -->
          <div class="mb-3">
            <label for="add-ind" class="form-label">指標資訊</label>
            <select class="form-select" id="add-ind" data-placeholder="選擇指標資訊" multiple>
              <option value="ind_01">2023 Q1銷售</option>
              <option value="ind_02">2023 Q1焦點產品銷售</option>
              <option value="ind_03">2023 Q2銷售</option>
              <option value="ind_04">2023 Q2焦點產品銷售</option>
              <option value="ind_05">2023 Q3銷售</option>
              <option value="ind_06">庫存週轉率（Inventory Turnover）</option>
              <option value="ind_07">庫存成本比率（Inventory Cost-to-Sales Ratio）</option>
              <option value="ind_08">安全庫存水平（Safety Stock Level）</option>
              <option value="ind_09">缺貨率（Stockout Rate）</option>
              <option value="ind_10">平均庫存週期（Average Inventory Days）</option>
              <option value="ind_11">周轉時間（Lead Time）</option>
              <option value="ind_12">品質控制（Quality Control）</option>
              <option value="ind_13">產能利用率（Capacity Utilization）</option>
              <option value="ind_14">交貨時間（Lead Time）</option>
              <option value="ind_15">平均收款週期（Average Collection Period）</option>
              <option value="ind_16">帳齡分析（Aging of Accounts Receivable）</option>
              <option value="ind_17">逾期帳款率（Accounts Receivable Overdue Percentage）</option>
              <option value="ind_18">應收帳款週轉率（Accounts Receivable Turnover）</option>
              <option value="ind_19">合約履行率（Contract Compliance）</option>
              <option value="ind_20">合約達成率（Contract Attainment）</option>
              <option value="ind_21">2023 Q3焦點產品銷售</option>
              <option value="ind_22">2023 Q4焦點產品銷售</option>
              <option value="ind_23">2023 Q4銷售</option>
            </select>
          </div>
          <!-- Table權限 -->
          <div class="mb-3">
            <label for="add-table" class="form-label">選擇Table權限</label>
            <select class="form-select" id="add-table" data-placeholder="選擇Table權限" multiple>
              <option value="table_01">charge</option>
              <option value="table_02">content_bonus</option>
              <option value="table_03">content_discount</option>
              <option value="table_04">coupon_master</option>
              <option value="table_05">fk_category_item</option>
              <option value="table_06">payment_agent</option>
              <option value="table_07">trans_bonus</option>
              <option value="table_08">trans_game_info_2017</option>
              <option value="table_09">trans_game_info_master</option>
              <option value="table_10">trans_master</option>
              <option value="table_11">coupon_trans_pin</option>
              <option value="table_12">user</option>
              <option value="table_13">trans_pin_2017</option>
              <option value="table_14">v_item_category</option>
              <option value="table_15">trans_2017</option>
            </select>
          </div>
    </div>
    <div class="modal-footer">
      <button type="button" class="btn" data-bs-dismiss="modal">取消</button>
      <button type="button" class="btn btn-primary" data-bs-dismiss="modal">確認</button>
    </div>
  </form>
</div>